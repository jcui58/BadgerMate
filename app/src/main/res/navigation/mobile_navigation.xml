<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.cs407.badgermate.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_dining"
        android:name="com.cs407.badgermate.ui.dining.DiningFragment"
        android:label="@string/title_dining"
        tools:layout="@layout/fragment_dining" />

    <fragment
        android:id="@+id/navigation_bus"
        android:name="com.cs407.badgermate.ui.bus.BusFragment"
        android:label="@string/title_bus"
        tools:layout="@layout/fragment_bus" />

    <fragment
        android:id="@+id/navigation_event"
        android:name="com.cs407.badgermate.ui.event.EventFragment"
        android:label="@string/title_event"
        tools:layout="@layout/fragment_bus" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.cs407.badgermate.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_bus" />
</navigation>